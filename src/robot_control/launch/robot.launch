<launch>
  <include file="$(find deedee_gazebo)/launch/deedee.launch"/>

  <node name="motors_driver"
        pkg="deedee_driver"
        type="motors_driver_sim"
        required="true"/>

  <node name="diff_drive_model"
        pkg="robot_control"
        type="diff_drive_model"
        required="true"/>

  <node name="controller"
        pkg="robot_control"
        type="controller"
        output="screen"
        required="false"/>

  <!--node name="teleops_node"
        pkg="teleop_twist_keyboard"
        type="teleop_twist_keyboard"
        required="false"/-->

</launch>
